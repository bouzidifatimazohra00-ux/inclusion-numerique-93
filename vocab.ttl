ipn:prenom a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .
ipn:age a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:int .
ipn:genre a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .
ipn:niveauEtudes a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .
ipn:programmeSuivi a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .
ipn:typeDispositif a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .
ipn:financeurProgramme a owl:DatatypeProperty ; rdfs:domain ipn:Apprenant ; rdfs:range xsd:string .

### Organisme Formation
ipn:typeOrganisme a owl:DatatypeProperty ; rdfs:domain ipn:OrganismeFormation ; rdfs:range xsd:string .
ipn:intituleFormation a owl:DatatypeProperty ; rdfs:domain ipn:OrganismeFormation ; rdfs:range xsd:string .
ipn:certification a owl:DatatypeProperty ; rdfs:domain ipn:OrganismeFormation ; rdfs:range xsd:string .
ipn:dureeHeures a owl:DatatypeProperty ; rdfs:domain ipn:OrganismeFormation ; rdfs:range xsd:int .
ipn:format a owl:DatatypeProperty ; rdfs:domain ipn:OrganismeFormation ; rdfs:range xsd:string .

### Entreprise
ipn:relationOrganisme a owl:DatatypeProperty ; rdfs:domain ipn:Entreprise ; rdfs:range xsd:string .
ipn:contactRH a owl:DatatypeProperty ; rdfs:domain ipn:Entreprise ; rdfs:range xsd:string .

### Structure Accompagnatrice
ipn:typeStructure a owl:DatatypeProperty ; rdfs:domain ipn:StructureAccompagnatrice ; rdfs:range xsd:string .
ipn:contactReferent a owl:DatatypeProperty ; rdfs:domain ipn:StructureAccompagnatrice ; rdfs:range xsd:string .
ipn:role a owl:DatatypeProperty ; rdfs:domain ipn:StructureAccompagnatrice ; rdfs:range xsd:string .

### Contrat
ipn:idContrat a owl:DatatypeProperty ; rdfs:domain ipn:Contrat ; rdfs:range xsd:string .
ipn:typeContrat a owl:DatatypeProperty ; rdfs:domain ipn:Contrat ; rdfs:range xsd:string .
ipn:dateDebut a owl:DatatypeProperty ; rdfs:domain ipn:Contrat ; rdfs:range xsd:date .
